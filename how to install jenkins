150  wget -q -O - https://pkg.jenkins.io/debian-stable/jenkins.io.key | sudo apt-key add -
  151  sudo sh -c 'echo deb https://pkg.jenkins.io/debian-stable binary/ > \
    /etc/apt/sources.list.d/jenkins.list'
  152  sudo apt-get update
  153  apt install openjdk-11-jdk
  154  apt-get install jenkins
  155  systemctl status jenkins
